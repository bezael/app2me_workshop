{
  "name": "app2me-workspace",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "run-p lib:test demo:test",
    "demo:start": "ng serve app2me",
    "demo:watch": "ng build app2me --watch --configuration development",
    "demo:test": "ng test app2me",
